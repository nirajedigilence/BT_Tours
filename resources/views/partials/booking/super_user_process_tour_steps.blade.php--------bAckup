			<script>
				$(document).on('click', '#downloadBrochureImage', function(e) { 
	
					e.preventDefault();
					var exp_id = <?php echo $item->id ;?> 
					var clsExp = 'downloadBrochureImage'+exp_id ; 
					console.log('exp_id'+clsExp);
					var rate_pp_new = $('.'+clsExp+' .rate_pp_new').val();
					var srs_pp_new = $('.srs_pp_new').val();
					 var brochure_tel = $('.brochure_tel').val();
					var brochure_email = $('.brochure_email').val();
					var brochure_email =  jQuery(".srpB").text();
					$('.loader').show();    
					$.ajax({
						url: base_url+'/download-brochure-image',
						type: 'POST',
						data: {'exp_id':exp_id, 'rate_pp_new':rate_pp_new, 'srs_pp_new':srs_pp_new, 'brochure_tel':brochure_tel, 'brochure_email':brochure_email},
						success: function(data) {
							console.log(data.response.status);
							if(data.response.status == "success"){
									console.log('1'+data.response.status);
								$('.downloadBrochureAppendCls').html(data.response);
								$('.loader').hide();  
								$('.downloadBrochurePriceContent').modal('hide');
								//$('#downloadBrochureModal'+<?php echo $item->id ;?>).modal('show');
								$('#downloadBrochureModal').modal('show');
							}else if(data.response.status == "fail"){
								console.log('2'+data.response.status);
								$('.loader').hide();
								$('.downloadBrochureAppendCls').html('Broucher data not found');
							}
						},
						error: function(e) {
						}
					});   
				});
			</script>
        @endforeach
    @endforeach
